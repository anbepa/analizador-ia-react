{
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "/api/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/gemini-proxy.js": {
            "maxDuration": 60
        }
    }
}